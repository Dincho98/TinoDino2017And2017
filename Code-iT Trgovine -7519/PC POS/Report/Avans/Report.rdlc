  <Code>
Public Function Model(model__1 As String) As String
	Dim rez As String = ""
	If model__1.Contains("¤") Then
		Dim mode As String() = model__1.Split("¤"C)
		rez = (If(mode(0).Length &gt; 0, "Model/Poziv na broj: " + mode(0), ""))

		If rez.Length &gt; 0 Then
			rez += Environment.NewLine
		End If

		rez += (If(mode(1).Length &gt; 0, "Storniran račun za predujam: " + mode(1), ""))


		Return rez
	End If


	Return rez
End Function


Public Function Datum(s As String, datum__1 As String) As String
	Dim d As String = ""
	Dim dd As New DateTime()
	If DateTime.TryParse(datum__1, dd) Then
		Return s &amp; dd.ToString("dd.MM.yyyy")
	End If
	Return d
End Function</Code>
